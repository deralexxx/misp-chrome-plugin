{
  "manifest_version": 2,

  "name": "MISP Chrome Plugin",
  "description": "This plugin will provide several actions to interact from the Browser with MISP",
  "version": "1.0",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },

  "content_scripts": [
    {
        "matches": ["<all_urls>"],
      "js": ["js/misp_interaction.js"]
    }
  ],

  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs"
  ]
}
